{
  "name": "food-del-api",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "babel-node ./src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "dev-test": "cross-env NODE_ENV=test nyc mocha --require @babel/register ./tests/*.js --timeout 8000 --exit",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --require @babel/register ./tests/*.js --timeout 8000 --exit",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrate": "npx sequelize-cli db:migrate",
    "prepare-db": "yarn drop-db && yarn create-db && yarn migrate && yarn create-admin && yarn seed",
    "pretest": "cross-env NODE_ENV=test yarn prepare-db",
    "heroku-postbuild": "yarn migrate && yarn create-admin",
    "create-admin": "babel-node ./src/database/scripts/adminScripts.js",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "make-runnable": "^1.3.10",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "sequelize": "^6.8.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-test-helpers": "^1.4.2",
    "twilio": "^3.70.0"
  },
  "devDependencies": {
    "@babel/register": "^7.15.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "mocha": "^9.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0"
  }
}
